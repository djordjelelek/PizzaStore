ems = Object.keys(props.ingredients).filter(\r\n    (el) => props.ingredients[el].show\r\n  );\r\n\r\n  const purchaseHanler = () => {\r\n    const day = new Date().getDay();\r\n    const mounth = new Date().getDate();\r\n    const year = new Date().getFullYear();\r\n    const hours = new Date().getHours();\r\n    const minutes = new Date().getMinutes();\r\n    const seconds = new Date().getMinutes();\r\n    const finalRecipe = {\r\n      recipe: listItems,\r\n      price: props.price,\r\n      date:\r\n        \"Date: \" +\r\n        day +\r\n        \".\" +\r\n        mounth +\r\n        \".\" +\r\n        year +\r\n        \". Time: \" +\r\n        hours +\r\n        \":\" +\r\n        minutes +\r\n        \":\" +\r\n        seconds,\r\n    };\r\n    axios\r\n      .post(\r\n        \"https://pizza-app-rg-default-rtdb.firebaseio.com/orders.json\",\r\n        finalRecipe\r\n      )\r\n      .then((response) => {\r\n        // if (alert(\"Your pizza is prepering!\")) {\r\n        // } else window.location.reload();\r\n        console.log(response);\r\n      });\r\n  };\r\n\r\n  return (\r\n    <div style={modalStyle} className={classes.paper}>\r\n      <h2>RECIPE</h2>\r\n      <ol>\r\n        <li>pizza: 130 RSD</li>\r\n        {listItems.map((ingr, index) => (\r\n          <li key={index}>\r\n            {ingr}: {props.ingredients[ingr].price} RSD\r\n          </li>\r\n        ))}\r\n      </ol>\r\n      <p>Total price: {props.price}</p>\r\n\r\n      <Button\r\n        variant=\"contained\"\r\n        color=\"primary\"\r\n        onClick={() => purchaseHanler()}\r\n        className={classes.button}\r\n      >\r\n        Buy\r\n      </Button>\r\n      <Button\r\n        variant=\"contained\"\r\n        color=\"secondary\"\r\n        onClick={() => props.setShowRecipe(false)}\r\n      >\r\n        Cancel\r\n      </Button>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Receipt;\r\n","C:\\Users\\Djordje\\VS Code\\JAVA_SCRIPT\\pizza-store\\src\\Header\\Menu\\Menu.