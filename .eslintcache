rn (\r\n    <Container className={classes.root}>\r\n      {Object.keys(props.ingredients).map((ingrident, index) => (\r\n        <div className={classes.buttons} key={index}>\r\n          {props.ingredients[ingrident].show ? (\r\n            <Button\r\n              variant=\"contained\"\r\n              color=\"primary\"\r\n              size=\"small\"\r\n              fullWidth\r\n              onClick={() => changeButtonHanler(ingrident, \"remove\")}\r\n            >\r\n              Remove {ingrident}\r\n            </Button>\r\n          ) : (\r\n            <Button\r\n              variant=\"contained\"\r\n              color=\"default\"\r\n              size=\"small\"\r\n              fullWidth\r\n              onClick={() => changeButtonHanler(ingrident, \"add\")}\r\n            >\r\n              Add {ingrident}\r\n            </Button>\r\n          )}\r\n        </div>\r\n      ))}\r\n      <h1 className={classes.Header}>\r\n        &nbsp;&nbsp;Current price: <strong>{price} RSD</strong>&nbsp;&nbsp;\r\n      </h1>\r\n      {/* <Receipt\r\n        ingredients={props.ingredients}\r\n        price={price}\r\n        setShowRecipe={setShowRecipe}\r\n        showRecipe={showRecipe}\r\n        handleClose={handleClose}\r\n      /> */}\r\n      {/* <Button\r\n        disabled={price === 130}\r\n        variant=\"contained\"\r\n        color=\"primary\"\r\n        size=\"large\"\r\n        // className={classes.OrderButton}\r\n        type=\"button\"\r\n        onClick={() => handleOpen()}\r\n      >\r\n        Order\r\n      </Button> */}\r\n      <SnackBar\r\n        price={price}\r\n        ingredients={props.ingredients}\r\n        setIngredients={props.setIngredients}\r\n      />\r\n    </Container>\r\n  );\r\n};\r\n\r\nexport default BuildControls;\r\n","C:\\Users\\Djordje\\VS Code\\JAVA_SCRIPT\\pizza-store\\src\\Main\\PizzaBuilder\\BuildControls\\Receipt\\Receipt.js",[],"C:\\Users\\Djordje\\VS Code\\JAVA_SCRIPT\\pizza-store\\src\\Header\\Menu\\Menu.js",[],"